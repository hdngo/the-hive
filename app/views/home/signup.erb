<form action="/bumblebee/new" method="post">
	<input type="text" name="username" placeholder="username">
	<input type="text" name="first_name" placeholder="First Name">
	<input type="text" name="last_name" placeholder="Last Name">
	<input type="password" name="password" placeholder="password">
	<input type="password_check" name="password_check" placeholder="please re-enter your password">
	<input type="text" name="email" placeholder="name@gmail.com">
	<input type="text" name="phone_number" placeholder="123-456-7890">
	<input type="submit" value="Submit">
</form>