<div class="body-div">

<ul class="list-group col-xs-6 col-xs-offset-3" id="results">
<p><%= "Your search results:" %></p>
<% @results.each do |result| %>
	<a href="/bumblebee/user/<%= result.username %>"><li class="list-group-item result"><span class="center-text"><%= full_name(result) + " (" + result.username + ")" %></span></li></a>
<% end %>
<% if @results.empty? %>
<li>No results found.</li>
	<% end %>
</ul>

</div>